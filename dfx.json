{
  "canisters": {
    "token": {
      "main": "motoko/token.mo",
      "type": "motoko"
    },
    "governance": { 
		  "candid": "rust/governance/governance.did",
		  "wasm": "target/wasm32-unknown-unknown/release/token.wasm",
		  "type": "custom",
      "build": [
        "cargo build --target wasm32-unknown-unknown --package governance --release",
        "ic-cdk-optimizer target/wasm32-unknown-unknown/release/dft_basic.wasm -o target/wasm32-unknown-unknown/release/dft_basic_opt.wasm"
      ]
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "dfx": "0.8.4",
    "networks": {
        "local": {
            "bind": "127.0.0.1:8000",
            "type": "ephemeral"
        }
    },
    "version": 1
}
